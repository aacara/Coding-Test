    "# n = 6\n",
    "# sequence = [10, 30, 10, 20, 20, 10]\n",
    "\n",
    "n = int(input())\n",
    "sequence =list(map(int, input().split()))\n",
    "d = [1] * (n+1)\n",
    "\n",
    "\n",
    " \n",
    "for i in range(n):\n",
    "    for j in range(i):\n",
    "        if sequence[i] < sequence[j]:\n",
    "            d[i] = max(d[i], d[j] + 1)\n",
    "\n",
    "print(max(d))"
   ]
  }
