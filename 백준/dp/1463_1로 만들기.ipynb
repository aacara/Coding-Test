   "source": [
    "n = int(input())\n",
    "dp = [0]*(n+1)\n",
    "\n",
    "for i in range(2, n+1):\n",
    "    dp[i] = dp[i-1] + 1\n",
    "    if i % 2 == 0:\n",
    "        dp[i] = min(dp[i//2]+1, dp[i])\n",
    "    if i % 3 == 0:\n",
    "        dp[i] = min(dp[i//3]+1, dp[i])\n",
    "\n",
    "print(dp[n])\n"
   ]
  }
