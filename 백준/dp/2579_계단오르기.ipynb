   "source": [
    "n = int(input())\n",
    "d = [0] * (n+4)\n",
    "stair = [0] * (n+4)\n",
    "\n",
    "# stair 값 입력\n",
    "for i in range(3, n+3):\n",
    "    stair[i] = int(input())\n",
    "\n",
    "\n",
    "# 총 점수의 최댓값\n",
    "for i in range(3, n+3):\n",
    "    d[i] = max(d[i-2] + stair[i], d[i-3] + stair[i-1] + stair[i])\n",
    "\n",
    "print(d[n+2])"
   ]
  }
